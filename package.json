    "pre-commit": "^1.2.2",
    "prettier": "^2.1.1",
    "serverless": "^2.1.1",
    "serverless-bundle": "^3.0.0",
    "serverless-dotenv-plugin": "^3.0.0"
  },
  "dependencies": {
    "@slack/events-api": "^2.3.4",
    "@slack/web-api": "^5.11.0",
    "words-to-numbers": "^1.5.1"
